{
  "name": "@naikamu/mobile",
  "version": "0.17.3",
  "codeVersion": "79",
  "apiVersion": "1.5.x",
  "private": true,
  "engines": {
    "node": "20.11.1"
  },
  "engineStrict": true,
  "scripts": {
    "android:staging": "react-native run-android --mode stagingdebug --appIdSuffix staging",
    "android:staging-release": "react-native run-android --mode stagingrelease --appIdSuffix staging",
    "android:dev": "react-native run-android --mode developmentdebug --appIdSuffix development",
    "android:dev-release": "react-native run-android --mode developmentrelease --appIdSuffix development",
    "android:prod": "react-native run-android --mode productiondebug",
    "android:prod-release": "react-native run-android --mode productionrelease",
    "ios": "react-native run-ios --mode \"Debug\" --scheme \"NaikamuDev\" --simulator \"iPhone 16\"",
    "ios:release": "react-native run-ios --mode \"Release\" --scheme \"NaikamuDev\" --simulator \"iPhone 15\"",
    "ios:stg:release": "react-native run-ios --mode \"Release\" --scheme \"NaikamuStg\"",
    "ios:prod:release": "react-native run-ios --mode \"Release\" --scheme \"Naikamu\"",
    "pod": "npx pod-install ios",
    "start": "react-native start --reset-cache",
    "start:clear": "cd android && ./gradlew clean && cd .. && react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "app:release": "cd android && ./gradlew bundleRelease",
    "apk:beta:release": "cd android && fastlane beta && cd .. && npm run apk:beta:rename && open android/app/build/outputs/apk/production/release",
    "apk:release:stg": "./release.sh -e stg",
    "apk:release:prod": "./release.sh -e prod",
    "apk:remove_assets": "rm -rf ./android/app/build/generated/assets"
  },
